{
  "name": "sama-backend",
  "version": "1.0.0",
  "description": "Sama Wellness Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "dev:all": "concurrently -n \"checkin-chat,backend,frontend\" -c \"cyan,green,blue\" \"npm run dev:checkin-chat\" \"npm run dev:backend-wait\" \"npm run dev:frontend-wait\"",
    "dev:checkin-chat": "cd ../sama-wellness-backend-main && python -m uvicorn app.main:app --reload --port 8000",
    "dev:backend-wait": "wait-on tcp:8000 && npm run dev",
    "dev:frontend-wait": "wait-on http://localhost:5000/api/health && cd ../frontend && npm run dev"
  },
  "keywords": [
    "wellness",
    "mental-health",
    "ayurveda",
    "chatbot"
  ],
  "author": "Sama Wellness Team",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/speech": "^7.2.1",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "form-data": "^4.0.5",
    "jsonwebtoken": "^9.0.2",
    "negotiator": "^1.0.0",
    "nodemailer": "^6.10.1",
    "openai": "^4.20.1",
    "pg": "^8.11.3",
    "socket.io": "^4.8.3",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.16.0",
    "@types/pumpify": "^1.4.5",
    "@types/supertest": "^6.0.3",
    "@types/web-push": "^3.6.4",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "fast-check": "^4.5.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "pg-mem": "^3.0.12",
    "supertest": "^7.2.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "wait-on": "^7.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
